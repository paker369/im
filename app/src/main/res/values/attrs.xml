<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LineBreakLayout">
        <attr name="HorizontalSpace" format="dimension"/>
        <!--标签行距-->
        <attr name="VerticalSpace" format="dimension" />
    </declare-styleable>

    <attr name="app_primary_color" format="color"/> <!-- topbar -->
    <attr name="app_content_bg_color" format="color"/> <!-- content-->

    <declare-styleable name="OptionItemView">
        <attr name="title" format="string" />
        <attr name="title_size" format="dimension" />
        <attr name="title_color" format="color" />
        <attr name="badge_count" format="integer" />
        <attr name="start_src" format="reference|color" />
        <attr name="end_src" format="reference|color" />
        <attr name="desc" format="string" />
        <attr name="desc_size" format="dimension" />
        <attr name="desc_color" format="color" />
        <attr name="show_arrow_indicator" format="boolean" />
        <attr name="divider_align_to_title" format="boolean" />
    </declare-styleable>
</resources>